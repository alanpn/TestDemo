<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/res_navigation"
    app:startDestination="@id/firstFragment">

    <fragment
        android:id="@+id/firstFragment"
        android:name="com.example.wubin.navigatormodule.NavigationFristFragment" />

    <fragment
        android:id="@+id/secondFragment"
        android:name="com.example.wubin.navigatormodule.NavigationSecondFragment" />

    <fragment
        android:id="@+id/thirdFragment"
        android:name="com.example.wubin.navigatormodule.NavigationThirdFragment" />

    <action
        android:id="@+id/to_firstFragment"
        app:destination="@id/firstFragment" />

    <action
        android:id="@+id/to_secondFragment"
        app:destination="@id/secondFragment" />

    <action
        android:id="@+id/to_thirdFragment"
        app:destination="@id/thirdFragment" />

</navigation>